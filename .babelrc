{
  "presets": [
    [
      "@babel/preset-env",
      { 
        // 给test和example文件使用
        "modules": "cjs"
      } 
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
